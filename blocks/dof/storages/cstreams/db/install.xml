<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="blocks/deansoffice/db" VERSION="20110921" COMMENT="XMLDB file for Moodle blocks/deansoffice"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="block_dof_s_cstreams" COMMENT="учебный поток">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" NEXT="ageid"/>
        <FIELD NAME="ageid" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="id учебного переиода" PREVIOUS="id" NEXT="programmitemid"/>
        <FIELD NAME="programmitemid" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="id дисциплины" PREVIOUS="ageid" NEXT="teacherid"/>
        <FIELD NAME="teacherid" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="id преподавателя по таблице persons" PREVIOUS="programmitemid" NEXT="departmentid"/>
        <FIELD NAME="departmentid" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="id подразделения, которому принадлежит учебный процесс" PREVIOUS="teacherid" NEXT="appointmentid"/>
        <FIELD NAME="appointmentid" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="Хранит id назначения на должность в таблице appointments" PREVIOUS="departmentid" NEXT="mdlgroup"/>
        <FIELD NAME="mdlgroup" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="id группы moodle, соответствующей учебному потоку" PREVIOUS="appointmentid" NEXT="eduweeks"/>
        <FIELD NAME="eduweeks" TYPE="int" LENGTH="3" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="количество учебных недель/циклов" PREVIOUS="mdlgroup" NEXT="begindate"/>
        <FIELD NAME="begindate" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="дата и время, когда учебный процесс был впервые переведен в состояние &quot;идет обучение&quot; по UTC" PREVIOUS="eduweeks" NEXT="enddate"/>
        <FIELD NAME="enddate" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="дата и время окончания по UTC, когда учебный процесс был в последний раз выведен из состояния &quot;идет обучения&quot;" PREVIOUS="begindate" NEXT="status"/>
        <FIELD NAME="status" TYPE="char" LENGTH="20" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="статус" PREVIOUS="enddate" NEXT="hours"/>
        <FIELD NAME="hours" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="количество часов отведенных для потока" PREVIOUS="status" NEXT="hoursweek"/>
        <FIELD NAME="hoursweek" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="количество часов в неделю" PREVIOUS="hours" NEXT="hoursweekinternally"/>
        <FIELD NAME="hoursweekinternally" TYPE="number" LENGTH="6" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" DECIMALS="2" COMMENT="часов в неделю очно" PREVIOUS="hoursweek" NEXT="name"/>
        <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="имя предмето-потока" PREVIOUS="hoursweekinternally" NEXT="hoursweekdistance"/>
		<FIELD NAME="hoursweekdistance" TYPE="number" LENGTH="6" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" DECIMALS="2" COMMENT="количество часов в неделю дистанционно" PREVIOUS="name" NEXT="explanatory" />
        <FIELD NAME="explanatory" TYPE="text" LENGTH="big" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="пояснительная записка" PREVIOUS="hoursweekdistance" NEXT="lastgradesync" />
		<FIELD NAME="lastgradesync" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="дата последней синхронизации оценок" PREVIOUS="explanatory" NEXT="salfactor"/>
      	<FIELD NAME="salfactor" TYPE="float" LENGTH="6" NOTNULL="true" DEFAULT="0" UNSIGNED="true" SEQUENCE="false" DECIMALS="2" ENUM="false" COMMENT="поправочный зарплатный коэффициент для учебного процесса" PREVIOUS="lastgradesync" NEXT="substsalfactor"/>
      	<FIELD NAME="substsalfactor" TYPE="float" LENGTH="6" NOTNULL="true" DEFAULT="0" UNSIGNED="true" SEQUENCE="false" DECIMALS="2" ENUM="false" COMMENT="замещающий поправочный коэффициент для учебного процесса" PREVIOUS="salfactor"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="iageid" UNIQUE="false" FIELDS="ageid" NEXT="iprogrammitemid"/>
        <INDEX NAME="iprogrammitemid" UNIQUE="false" FIELDS="programmitemid" PREVIOUS="iageid" NEXT="iteacherid"/>
        <INDEX NAME="iteacherid" UNIQUE="false" FIELDS="teacherid" PREVIOUS="iprogrammitemid" NEXT="idepartmentid"/>
        <INDEX NAME="idepartmentid" UNIQUE="false" FIELDS="departmentid" PREVIOUS="iteacherid" NEXT="imdlgroup"/>
        <INDEX NAME="imdlgroup" UNIQUE="false" FIELDS="mdlgroup" PREVIOUS="idepartmentid" NEXT="ibegindate"/>
        <INDEX NAME="ibegindate" UNIQUE="false" FIELDS="begindate" PREVIOUS="imdlgroup" NEXT="ienddate"/>
        <INDEX NAME="ienddate" UNIQUE="false" FIELDS="enddate" PREVIOUS="ibegindate" NEXT="istatus"/>
        <INDEX NAME="istatus" UNIQUE="false" FIELDS="status" PREVIOUS="ienddate" NEXT="mdl_blocdofscstr_app_ix"/>
        <INDEX NAME="mdl_blocdofscstr_app_ix" UNIQUE="false" FIELDS="appointmentid" PREVIOUS="istatus" NEXT="ilastgradesync"/>
		<INDEX NAME="ilastgradesync" UNIQUE="false" FIELDS="lastgradesync" PREVIOUS="mdl_blocdofscstr_app_ix" NEXT="ihoursweekinternally"/>
		<INDEX NAME="ihoursweekinternally" UNIQUE="false" FIELDS="hoursweekinternally" PREVIOUS="ilastgradesync" NEXT="isalfactor"/>        
      	<INDEX NAME="isalfactor" UNIQUE="false" FIELDS="salfactor" PREVIOUS="ihoursweekinternally" NEXT="isubstsalfactor"/>
      	<INDEX NAME="isubstsalfactor" UNIQUE="false" FIELDS="substsalfactor" PREVIOUS="isalfactor"/>
      </INDEXES>
    </TABLE>
  </TABLES>
</XMLDB>
<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="blocks/deansoffice/db" VERSION="20090506" COMMENT="XMLDB file for Moodle blocks/deansoffice"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="block_dof_s_ages" COMMENT="Учебные периоды">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" NEXT="name"/>
        <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="название учебного периода" PREVIOUS="id" NEXT="begindate"/>
        <FIELD NAME="begindate" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="дата и время начала по UTC" PREVIOUS="name" NEXT="enddate"/>
        <FIELD NAME="enddate" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="дата и время окончания по UTC" PREVIOUS="begindate" NEXT="eduweeks"/>
        <FIELD NAME="eduweeks" TYPE="int" LENGTH="3" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="количество учебных недель/циклов" PREVIOUS="enddate" NEXT="departmentid"/>
        <FIELD NAME="departmentid" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="структурное подразделение, объявившее учебный период" PREVIOUS="eduweeks" NEXT="previousid"/>
        <FIELD NAME="previousid" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="предыдущий учебный период" PREVIOUS="departmentid" NEXT="status"/>
        <FIELD NAME="status" TYPE="char" LENGTH="20" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="список статусов" PREVIOUS="previousid"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="iname" UNIQUE="false" FIELDS="name" NEXT="ibegindate"/>
        <INDEX NAME="ibegindate" UNIQUE="false" FIELDS="begindate" PREVIOUS="iname" NEXT="ienddate"/>
        <INDEX NAME="ienddate" UNIQUE="false" FIELDS="enddate" PREVIOUS="ibegindate" NEXT="idepartmentid"/>
        <INDEX NAME="idepartmentid" UNIQUE="false" FIELDS="departmentid" PREVIOUS="ienddate" NEXT="ipreviousid"/>
        <INDEX NAME="ipreviousid" UNIQUE="false" FIELDS="previousid" PREVIOUS="idepartmentid" NEXT="istatus"/>
        <INDEX NAME="istatus" UNIQUE="false" FIELDS="status" PREVIOUS="ipreviousid"/>
      </INDEXES>
    </TABLE>
  </TABLES>
</XMLDB>
<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="blocks/deansoffice/db" VERSION="20110711" COMMENT="XMLDB file for Moodle blocks/deansoffice"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="block_dof_s_schtemplates" COMMENT="расписание учебной недели">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" NEXT="cstreamid"/>
        <FIELD NAME="cstreamid" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="id учебного потока" PREVIOUS="id" NEXT="daynum"/>
        <FIELD NAME="daynum" TYPE="int" LENGTH="1" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="номер дня недели" PREVIOUS="cstreamid" NEXT="dayvar"/>
        <FIELD NAME="dayvar" TYPE="int" LENGTH="1" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="вариант дня" PREVIOUS="daynum" NEXT="type"/>
        <FIELD NAME="type" TYPE="char" LENGTH="20" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="тип события" PREVIOUS="dayvar" NEXT="begin"/>
        <FIELD NAME="begin" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="время начала события в секундах от 00:00 по UTC" PREVIOUS="type" NEXT="duration"/>
        <FIELD NAME="duration" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="продолжительность события" PREVIOUS="begin" NEXT="place"/>
        <FIELD NAME="place" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="место проведения события" PREVIOUS="duration" NEXT="form"/>
        <FIELD NAME="form" TYPE="char" LENGTH="20" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="тип урока (очно/дистанционно)" PREVIOUS="place" NEXT="departmentid"/>
        <FIELD NAME="departmentid" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="id структурного подразделения" PREVIOUS="form" NEXT="status"/>
        <FIELD NAME="status" TYPE="char" LENGTH="20" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="статус" PREVIOUS="departmentid" NEXT="salfactor"/>
      	<FIELD NAME="salfactor" TYPE="float" LENGTH="6" NOTNULL="true" DEFAULT="1" UNSIGNED="true" SEQUENCE="false" DECIMALS="2" ENUM="false" COMMENT="поправочный зарплатный коэффициент для шаблона расписания" PREVIOUS="status"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="icstreamid" UNIQUE="false" FIELDS="cstreamid" NEXT="idaynum"/>
        <INDEX NAME="idaynum" UNIQUE="false" FIELDS="daynum" PREVIOUS="icstreamid" NEXT="idayvar"/>
        <INDEX NAME="idayvar" UNIQUE="false" FIELDS="dayvar" PREVIOUS="idaynum" NEXT="itype"/>
        <INDEX NAME="itype" UNIQUE="false" FIELDS="type" PREVIOUS="idayvar" NEXT="ibegin"/>
        <INDEX NAME="ibegin" UNIQUE="false" FIELDS="begin" PREVIOUS="itype" NEXT="iduration"/>
        <INDEX NAME="iduration" UNIQUE="false" FIELDS="duration" PREVIOUS="ibegin" NEXT="iform"/>
        <INDEX NAME="iform" UNIQUE="false" FIELDS="form" PREVIOUS="iduration" NEXT="idepartmentid"/>
        <INDEX NAME="idepartmentid" UNIQUE="false" FIELDS="departmentid" PREVIOUS="iform" NEXT="istatus"/>
        <INDEX NAME="istatus" UNIQUE="false" FIELDS="status" PREVIOUS="idepartmentid" NEXT="isalfactor"/>
      	<INDEX NAME="isalfactor" UNIQUE="false" FIELDS="salfactor" PREVIOUS="istatus"/>
      </INDEXES>
    </TABLE>
  </TABLES>
</XMLDB>
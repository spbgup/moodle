<?xml version="1.0" encoding="utf-8"?>
<XMLDB PATH="blocks/deansoffice/db" VERSION="20110124" COMMENT="XMLDB file for Moodle blocks/deansoffice"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="block_dof_s_agrouphistory" COMMENT="таблица хранит история смены учебных периодов для групп">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" NEXT="agroupid"/>
        <FIELD NAME="agroupid" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="id подписки ученика на учебную программу в таблице programmsbcs." PREVIOUS="id" NEXT="ageid"/>
        <FIELD NAME="ageid" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="id учебного периода в таблице ages" PREVIOUS="agroupid" NEXT="agenum"/>
        <FIELD NAME="agenum" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="номер ступени обучения. (второй класс, четвертый курс, и т. п.)" PREVIOUS="ageid" NEXT="changedate"/>
        <FIELD NAME="changedate" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="дата создания записи в формате unixtime" PREVIOUS="agenum" NEXT="orderid"/>
        <FIELD NAME="orderid" TYPE="int" LENGTH="11" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="id приказа, в результате которого была изменена группа (таблица orders). Возможно null-значение, если изменения не были связаны ни с каким приказом." PREVIOUS="changedate"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="iagroupid" UNIQUE="false" FIELDS="agroupid" NEXT="iageid"/>
        <INDEX NAME="iageid" UNIQUE="false" FIELDS="ageid" PREVIOUS="iagroupid" NEXT="iagenum"/>
        <INDEX NAME="iagenum" UNIQUE="false" FIELDS="agenum" PREVIOUS="iageid" NEXT="ichangedate"/>
        <INDEX NAME="ichangedate" UNIQUE="false" FIELDS="changedate" PREVIOUS="iagenum" NEXT="iorderid"/>
        <INDEX NAME="iorderid" UNIQUE="false" FIELDS="orderid" PREVIOUS="ichangedate"/>
      </INDEXES>
    </TABLE>
  </TABLES>
</XMLDB>
<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="blocks/deansoffice/db" VERSION="20090615" COMMENT="XMLDB file for Moodle blocks/deansoffice"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="block_dof_s_schpresences" COMMENT="содержит сведения о присутствии или отсутствии учащихся на учебном событии">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" NEXT="eventid"/>
        <FIELD NAME="eventid" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="id события" PREVIOUS="id" NEXT="personid"/>
        <FIELD NAME="personid" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="id персоны" PREVIOUS="eventid" NEXT="present"/>
        <FIELD NAME="present" TYPE="char" LENGTH="10" NOTNULL="false" SEQUENCE="false" ENUM="true" ENUMVALUES="'0', '1'" COMMENT="присутствие" PREVIOUS="personid" NEXT="orderid"/>
        <FIELD NAME="orderid" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="id приказа, в соответствии в котором был выставлен статус присутсвия" PREVIOUS="present"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="ieventid" UNIQUE="false" FIELDS="eventid" NEXT="ipersonid"/>
        <INDEX NAME="ipersonid" UNIQUE="false" FIELDS="personid" PREVIOUS="ieventid" NEXT="ipresent"/>
        <INDEX NAME="ipresent" UNIQUE="false" FIELDS="present" PREVIOUS="ipersonid" NEXT="iorderid"/>
        <INDEX NAME="iorderid" UNIQUE="false" FIELDS="orderid" PREVIOUS="ipresent"/>
      </INDEXES>
    </TABLE>
  </TABLES>
</XMLDB>
<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="blocks/deansoffice/db" VERSION="20120307" COMMENT="XMLDB file for Moodle blocks/deansoffice"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="block_dof_s_departments" COMMENT="Подразделения учебного заведения">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" NEXT="name"/>
        <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="название" PREVIOUS="id" NEXT="code"/>
        <FIELD NAME="code" TYPE="char" LENGTH="20" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="кодовое обозначение" PREVIOUS="name" NEXT="managerid"/>
        <FIELD NAME="managerid" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="руководитель структурного подразделения" PREVIOUS="code" NEXT="leaddepid"/>
        <FIELD NAME="leaddepid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="вышестоящее структурное подразделение" PREVIOUS="managerid" NEXT="addressid"/>
        <FIELD NAME="addressid" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="местоположение" PREVIOUS="leaddepid" NEXT="zone"/>
        <FIELD NAME="zone" TYPE="char" LENGTH="100" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="временная зона, действующая в подразделении" PREVIOUS="addressid" NEXT="status"/>
        <FIELD NAME="status" TYPE="char" LENGTH="20" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="статус" PREVIOUS="zone" NEXT="path"/>
		<FIELD NAME="path" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="список всех родительских подразделений" PREVIOUS="status" NEXT="depth"/>
        <FIELD NAME="depth" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="глубина вложенности подразделения" PREVIOUS="path"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="iname" UNIQUE="false" FIELDS="name" NEXT="icode"/>
        <INDEX NAME="icode" UNIQUE="false" FIELDS="code" PREVIOUS="iname" NEXT="imanagerid"/>
        <INDEX NAME="imanagerid" UNIQUE="false" FIELDS="managerid" PREVIOUS="icode" NEXT="ileaddepid"/>
        <INDEX NAME="ileaddepid" UNIQUE="false" FIELDS="leaddepid" PREVIOUS="imanagerid" NEXT="iaddressid"/>
        <INDEX NAME="iaddressid" UNIQUE="false" FIELDS="addressid" PREVIOUS="ileaddepid" NEXT="izone"/>
        <INDEX NAME="izone" UNIQUE="false" FIELDS="zone" PREVIOUS="iaddressid" NEXT="istatus"/>
        <INDEX NAME="istatus" UNIQUE="false" FIELDS="status" PREVIOUS="izone" NEXT="ipath"/>
		<INDEX NAME="ipath" UNIQUE="false" FIELDS="path" PREVIOUS="istatus" NEXT="idepth"/>
		<INDEX NAME="idepth" UNIQUE="false" FIELDS="depth" PREVIOUS="ipath"/>
      </INDEXES>
    </TABLE>
  </TABLES>
</XMLDB>
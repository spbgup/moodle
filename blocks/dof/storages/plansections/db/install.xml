<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="blocks/deansoffice/db" VERSION="20101216" COMMENT="XMLDB file for Moodle blocks/deansoffice"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="block_dof_s_plansections" COMMENT="Таблица тематических разделов">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" NEXT="linkid"/>
        <FIELD NAME="linkid" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="связь с учебным периодом, учебной программой, дисциплиной или учебным процессом" PREVIOUS="id" NEXT="linktype"/>
        <FIELD NAME="linktype" TYPE="char" LENGTH="20" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="тип связи" PREVIOUS="linkid" NEXT="name"/>
        <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="название тематического раздела" PREVIOUS="linktype" NEXT="status"/>
		<FIELD NAME="status" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="статус тематического раздела" PREVIOUS="name" />
     </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="ilinkid" UNIQUE="false" FIELDS="linkid" NEXT="ilinktype"/>
        <INDEX NAME="ilinktype" UNIQUE="false" FIELDS="linktype" PREVIOUS="ilinkid" NEXT="iname"/>
        <INDEX NAME="iname" UNIQUE="false" FIELDS="name" PREVIOUS="ilinktype" NEXT="istatus"/>
		<INDEX NAME="istatus" UNIQUE="false" FIELDS="status" PREVIOUS="iname"/>
      </INDEXES>
    </TABLE>
  </TABLES>
</XMLDB>
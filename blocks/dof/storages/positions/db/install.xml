<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="blocks/deansoffice/db" VERSION="20100402" COMMENT="XMLDB file for Moodle blocks/deansoffice"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../../../lib/xmldb/xmldb.xsd"
>
    <TABLES>
        <TABLE NAME="block_dof_s_positions" COMMENT="Список должностей">
        <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" NEXT="name"/>
        <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="название должности" PREVIOUS="id" NEXT="code"/>
        <FIELD NAME="code" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="Код должности" PREVIOUS="name" NEXT="departmentid"/>
        <FIELD NAME="departmentid" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="id подразделения, в котором числится должность" PREVIOUS="code" NEXT="status"/>
        <FIELD NAME="status" TYPE="char" LENGTH="20" NOTNULL="false" SEQUENCE="false" ENUM="false" PREVIOUS="departmentid"/>
        </FIELDS>
        <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
        </KEYS>
        <INDEXES>
        <INDEX NAME="mdl_blocdofsposi_nam_ix" UNIQUE="false" FIELDS="name" NEXT="mdl_blocdofsposi_cod_ix"/>
        <INDEX NAME="mdl_blocdofsposi_cod_ix" UNIQUE="false" FIELDS="code" PREVIOUS="mdl_blocdofsposi_nam_ix" NEXT="mdl_blocdofsposi_dep_ix"/>
        <INDEX NAME="mdl_blocdofsposi_dep_ix" UNIQUE="false" FIELDS="departmentid" PREVIOUS="mdl_blocdofsposi_cod_ix" NEXT="mdl_blocdofsposi_sta_ix"/>
        <INDEX NAME="mdl_blocdofsposi_sta_ix" UNIQUE="false" FIELDS="status" PREVIOUS="mdl_blocdofsposi_dep_ix"/>
        </INDEXES>
        </TABLE>
    </TABLES>
</XMLDB>